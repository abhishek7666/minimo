*{
	padding:0px;
	margin:0px;
	box-sizing:border-box;
}
::selection{
	background-color:hotpink;
}
/* helper class */
a:link,
a:hover,
a:focus,
a:active{
	text-decoration:none;
}
.text-center{
	text-align:center;
}
.container{
	width:1060px;
	margin:0 auto;
}
.row,
.row::after,
.row::before{
	clear:both;
	display:table;
	width:100%;
}
/*.row{
	margin-left:-15px;
	margin-right:-15px;
}*/
.row>:first-child{
	padding-left:0px;
}
.row>:last-child{
	padding-right:0px;
}
.col3, .col6, .col9, .col10{
	float:left;
	padding:0 15px;
}
.col3{
	width:25%;
}
.col9{
	width:75%;
}
.col10{
	width:calc((100% * 10) / 12);
}
.col12{
	width:100%;
	
}
.col6{
	width:50%;
}
.text-right{
	text-align:right;
}
.mb-70{
	margin-bottom:70px;
}
.mb-100{
	margin-bottom:100px;
}
.ml-oneGrid{
	margin-left:calc(100% / 12);
	}
.mb-55{
	margin-bottom:55px;
}
.mb-35{
	margin-bottom:35px;
}
.mb-70{
	margin-bottom:70px;
}
.mb-50{
	margin-bottom:50px;
}
/* Hero image section */

.heroImg{
	padding:70px 0 100px;
}
.logo{
	font-size:30px;
	text-transform:uppercase;
	color:#000000;
}
.navbar{
	list-style:none;
	text-align:right;
}
.navbar li{
	display:inline-block;
	margin:0 12.5px;
}
.navbar li:last-child{
	margin-right:0;
}
.navbar li a{
	color:#626262;
	font-size:14px;
	text-transform:uppercase;
	padding:10px;
	display:inline-block;
}
.navbar li:last-child a{
	padding-right:0;
}
.sectionLabel{
	color:#b4ad9e;
	font-size:14px;
	text-transform:uppercase;
	margin-bottom:30px;
}
.sectionHeadin{
	font-size:30px;
	color:#626262;
	margin-bottom:35px;
}
.info{
	color:#626262;
	font-size:14px;
	line-height:19px;
	
}
.thumbsction{
	padding-top:105px;
}
.addressSection {
	padding:70px 0 65px;
	background-color:#f0f0f0;
}
.newform{
	max-width:400px;
	margin:0 auto;
	border-bottom:2px solid #000;
	position:relative;
	
	
}
.newform input{
	border:none;
	background-color:transparent;
	font-size:14px;
	color:#000000;
	font-weight:bold;
	height:40px;
	 padding:0 5px;
	 width:100%;
}
.newform i{
	position:absolute;
	right:12px;
	top:11px;
	transform:rotate(45deg);
	font-size:18px;
}
.load-morebutton a{
	border:1px solid #000;
	text-transform:uppercase;
	color:#626262;
	font-size:20px;
	padding:25px 30px;
	display:inline-block;
}
.footerarea{
	background-color:#f0f0f0;
	padding:25px 0px;
	
}
.footertext{
	overflow:hidden;
}
.footer-left{
	float:left;
	
}
.footer-left p a{
	color:#000;
	font-size:14px;
	margin-right:20px
}
.footer-right{
	float:right;
	
}
.footer-right a{
	color:#000;
	margin-left:13px;
}